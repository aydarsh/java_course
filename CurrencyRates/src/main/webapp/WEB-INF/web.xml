<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns="http://xmlns.jcp.org/xml/ns/javaee"
         xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd"
         version="4.0">
    <servlet>
        <servlet-name>latest-rate</servlet-name>
        <servlet-class>com.rostertwo.servlets.LatestRateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>latest-rate</servlet-name>
        <url-pattern>/latest</url-pattern>
    </servlet-mapping>

    <servlet>
        <servlet-name>archived-rate</servlet-name>
        <servlet-class>com.rostertwo.servlets.ArchivedRateServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>archived-rate</servlet-name>
        <url-pattern>/archive</url-pattern>
    </servlet-mapping>
</web-app>